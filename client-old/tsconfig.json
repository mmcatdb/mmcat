{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [ "env.d.ts", "src/**/*", "src/**/*.vue" ],
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es2019",
            "DOM"
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": [ "./src/*" ]
        },
        "types": [
            "@vue-macros/reactivity-transform/macros-global",
        ]
    },
    "references": [
        {
            "path": "./tsconfig.vite-config.json"
        }
    ]
}
